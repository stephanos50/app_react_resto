'successproductReview')\n            setRating(0)\n            setComment('')\n            \n        }\n        if (!product.id || product.id !== match.params.id) {\n            console.log('je suis reset')\n            dispatch({ type: PRODUCT_CREATE_REVIEW_RESET})\n            dispatch(listProductDetails(match.params.id))\n        }\n    }, [dispatch,match, successProductReview])\n   \n    if (!product.pictures ) return null;\n    if (!product.price ) return null;\n\n    const addToCartHandler = () => {\n        history.push(`/cart/${match.params.id}?qty=${qty}`)\n    }\n\n    const picture = product.pictures.map((image) => image.path)\n\n    const submitHandler = (e) => {\n        e.preventDefault()\n        dispatch(createProductReview(match.params.id, {\n            rating,\n            comment,\n        }))\n    }\n    \n   \n    \n    return ( \n        <>\n       \n        <Link to='/' className='pb-4'>\n            <Button variant=\"primary\">Retour</Button>\n        </Link>\n        {loading ? (\n            <Loader />\n        ) : error ? (\n            <Message variant='danger'>{error}</Message>\n        ) : (\n            <>  \n            <Row>\n               \n                <Col key={picture} sm={12} md={6} lg={4} xl={3} className=\"p-3\">\n                    <Image  style={{ width: '12rem' }} src={picture}  />\n                </Col>\n                    <Col className=\"p-3\" sm={12} md={6} lg={4} xl={4} >\n                        <ListGroup variant='flush'>\n                            <h4>{product.name}</h4>\n                            \n                            <h6> Prix : {product.price} euro</h6>\n                        </ListGroup>\n                        <div className='product_assess'><Assess value={product.cote} /></div>\n                    </Col>\n                   \n                    <Col className=\"p-3\" sm={12} md={6} lg={4} xl={3}>\n                        <Card>\n                            <ListGroup variant='flush'>\n                                <ListGroup.Item>\n                                    <Row>\n                                        <Col> Price: </Col>\n                                        <Col> <strong>{product.price}</strong> euro </Col>\n                                    </Row>\n                                </ListGroup.Item>\n\n                                <ListGroup.Item>\n                                    <Row>\n                                        <Col>Quantité</Col>\n                                        <Col>\n                                            <Form.Control \n                                                as='select' \n                                                value={qty} \n                                                onChange={(e) => setQty(e.target.value)}>\n                                                    {\n                                                        [ ...Array(quantities.length).keys()].map((x) =>(\n                                                            <option key={x + 1 } value={x + 1 }> {x + 1 } </option>\n                                                            ))\n                                                    }\n                                            </Form.Control>\n                                        </Col>\n                                    </Row>\n                                </ListGroup.Item>\n\n                                <ListGroup.Item>\n                                    <Button \n                                        onClick={addToCartHandler}\n                                            className='btn'\n                                            type='button'\n                                        >Ajouter au panier</Button>\n                                </ListGroup.Item>\n                            </ListGroup>\n                            </Card>\n                        </Col>\n                    </Row>\n                \n                    <Row>\n                        <Col md={6} className=\"p-3\">\n                            <h4>Déscription</h4>\n                            <p>{product.description}</p>\n                        </Col>\n                    </Row>\n\n                    <Row>\n                        <Col md={6} className=\"p-3\">\n                            <h4>Allergene</h4>\n                            {\n                                product.allergens.map((item) => <i key={item.id}> {item.name} </i>)\n                            }\n                           \n                            \n                        </Col>\n                    </Row>\n                    <Row>\n                        <Col md={6}>\n                            <h2>Reviews</h2>\n                            {Object.keys(product.reviews).length === 0 && <Message>No reviews</Message>}\n                            <ListGroup variant='flush'>\n                                {product.reviews.map((review) => (\n                                    <ListGroup.Item key={review.id}>\n                                        <strong>{review.name}</strong>\n                                        <Assess value={review.rate}/>\n                                        <p>{review.createdAt.substring(0, 10)}</p>\n                                        <p>{review.comment}</p>\n                                    </ListGroup.Item>    \n                                ))}\n                            </ListGroup>\n                            <ListGroup>\n                                {successProductReview && (\n                                    <Message variant='success'>\n                                    Review submitted successfully\n                                    </Message>\n                                )}\n                                {loadingProductReview && <Loader />}\n                               \n                                {errorproductReview && <Message variant='danger'>{errorproductReview}</Message>}\n                                { userInfo && ( \n                                   \n                                      <Form onSubmit= {submitHandler}>\n                                          <Form.Group controlId='rating'>\n                                              <Form.Label>Rating</Form.Label>\n                                              <Form.Control \n                                                as='select' \n                                                value={rating}\n                                                onChange={(e) => setRating(e.target.value)}>\n                                                    <option value=''>Select...</option>\n                                                    <option value='1'>1 - Poor</option>\n                                                    <option value='2'>2 - Fair</option>\n                                                    <option value='3'>3 - Good</option>\n                                                    <option value='4'>4 - Very Goog</option>\n                                                    <option value='5'>5 - Excellent</option>\n                                                </Form.Control>\n                                          </Form.Group>\n                                          <Form.Group>\n                                                <Form.Label>Comment</Form.Label>\n                                                <Form.Control \n                                                    as='textarea' \n                                                    row='3' \n                                                    value={comment}\n                                                    onChange={(e) => setComment(e.target.value)}>\n                                                </Form.Control>\n                                          </Form.Group>\n                                          <Button type='submit' variant='primary' >Submit</Button>\n                                      </Form>\n                                 )}\n                            </ListGroup>\n                        </Col>\n                    </Row>\n                </>\n            )}\n        </>\n    )\n}\n\nexport default Product\n","/Users/stefanos/Documents/web_app_coeurbleu/frontend/src/views/CartScreen.js",[],"/Users/stefanos/Documents/web_app_coeurbleu/frontend/src/views/LoginScreen.js",[],"/Users/stefanos/Documents/web_app_coeurbleu/frontend/src/views/RegisterScreen.js",[],"/Users/stefanos/Documents/web_app_coeurbleu/frontend/src/views/ProfileScreen.js",[],"/Users/stefanos/Documents/web_app_coeurbleu/frontend/src/views/ShippingScreen.js",["458"],"import { useState, useEffect } from 'react'\nimport { Redirect } from 'react-router-dom'\nimport { Form, Button, Col , Row} from 'react-bootstrap'\nimport { useDispatch, useSelector } from 'react-redux'\nimport FormContainer  from '../composants/FormContainer'\nimport { saveShippingAddress} from '../actions/cartAction'\nimport CheckoutSteps  from '../composants/CheckoutSteps'\nimport {listCities } from '../actions/cityAction'\n\n\nconst ShippingScreem = ({ history }) => {\n\n    const cart = useSelector((state) => state.cart)\n    const cityList = useSelector(state => state.cityList)\n    // eslint-disable-next-line\n    const { loading, error, cities} = cityList\n\n    const {shippingAddress} = cart\n\n    const userLogin = useSelector((state) => state.userLogin)\n    const { userInfo } = userLogin\n\n   \n   \n    const [address, setAddress] = useState(userInfo.address.name|| '')\n    const [number, setNumber] = useState(userInfo.address.number || '')\n    const [floor, setFloor] = useState(userInfo.address.floor || '')\n    const [city, setCity] = useState(userInfo.address.cityId || {})\n    \n\n    const dispatch = useDispatch()\n\n    useEffect(() => {\n        dispatch(listCities())\n        \n        setCity({\n            id:1,\n            name:'Audergem',\n            zip:'1160',\n        })\n       \n    }, [dispatch])\n\n    \n   \n    const sumbitHandler = (e) => {\n        e.preventDefault()\n        dispatch(saveShippingAddress({address, number, floor, city }))\n        history.push('/payment')\n    }\n\n    const setCityName = (city) => {\n        cities.map((item) => item.name === city &&  setCity(\n           {\n               id: item.id,\n               name: item.name,\n               zip: item.zip,\n           }\n       ))\n    }\n\n    return <FormContainer>\n        <CheckoutSteps step1 step2 />\n           <h1>Shipping</h1>\n           <Form onSubmit={sumbitHandler}>\n               <Row>\n                   <Col md={12}>\n                        <Form.Group className=\"mb-3\" controlId='address'>\n                            <Form.Label>Adresse</Form.Label>\n                            <Form.Control\n                                type='text'\n                                placeholder='Entrez votre adresse'\n                                value={address}\n                                required\n                                onChange={(e) => setAddress(e.target.value)}\n                            ></Form.Control>\n                        </Form.Group>\n                    </Col>\n                </Row>\n                <Row>\n                    <Col md={4}>\n                        <Form.Group className=\"mb-3\" controlId='number'>\n                            <Form.Label>Numéro</Form.Label>\n                            <Form.Control\n                                type='number'\n                                placeholder='Votre numéro'\n                                value={number}\n                                required\n                                onChange={(e) => setNumber(e.target.value)}\n                            ></Form.Control>\n                        </Form.Group>\n                    </Col>\n                    <Col md={4}>\n                        <Form.Group className=\"mb-3\" controlId='floor'>\n                            <Form.Label>Etage</Form.Label>\n                            <Form.Control\n                                type='text'\n                                placeholder='Votre étage'\n                                required\n                                value={floor}\n                                onChange= {\n                                    (e) => \n                                    {\n                                        if(e.target.value >= 0) {\n                                            setFloor(e.target.value) \n                                        }\n                                    }\n                                       \n                                }\n                            ></Form.Control>\n                        </Form.Group>\n                    </Col>\n               \n                    <Col md={4}>\n                        <Form.Group className=\"mb-3\" controlId='city'> \n                            <Form.Label>Commune</Form.Label>\n                            <Form.Select aria-label=\"\" onChange={(e) => setCityName(e.target.value)  }>\n                                { cities !== undefined ? \n                                    \n                                    cities.map((item) => (\n                                        \n                                     <option key={item.id}  value={item.name}>\n                                         {item.name}\n                                     </option>\n                                 ))\n                                   \n                             :  <Redirect to='/cart' /> }\n                                \n                            </Form.Select>\n                        </Form.Group>\n                    </Col>\n                </Row>\n                <Row>\n                    <Col>\n                        <Button type='submit' variant='primary'>Continuer</Button>\n                    </Col>\